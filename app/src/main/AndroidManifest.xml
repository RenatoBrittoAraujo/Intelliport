<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.intelliport">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/intelliport_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CustomDelivery"></activity>
        <activity android:name=".NewDelivery" />
        <activity android:name=".NewDeliveryLog" />
        <activity android:name=".LearnMore" />
        <activity android:name=".NewVehicle" />
        <activity android:name=".NewDriver" />
        <activity android:name=".Login" />
        <activity android:name=".Register" />
        <activity android:name=".delivery_layout" />
        <activity
            android:name=".MainPanel"
            android:label="@string/title_activity_dashboard" />
        <activity android:name=".Signin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>